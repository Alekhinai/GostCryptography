<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>GostCryptography</id>
    <title>GostCryptography</title>
    <description>
      .NET driver for ViPNet CSP and CryptoPro CSP. Implements crypto algorithms
      based on Russian national cryptographic standards GOST 28147-89, GOST R 34.10
      and GOST R 34.11. Also provides abstractions to sign and verify CMS/PKCS #7
      messages, sign, verify and encrypt XML documents.
    </description>
    <version>2.0.4</version>
    <authors>Alexander Mezhov</authors>
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <language>en-US</language>
    <tags>GOST GOST-2012 Cryptography ViPNet CryptoPro</tags>
    <projectUrl>https://github.com/AlexMAS/GostCryptography</projectUrl>
    <repository type="git" url="https://github.com/AlexMAS/GostCryptography" />
  </metadata>
</package>